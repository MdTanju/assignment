            }


        });


        $('#dsitmoney').change(function() {
            var value = $(this).val();
            if (value.indexOf(',') > -1) {
                value = value.replace(/\,/g, '');
            }

            value = parseInt(value.replace(" BDT", ""));


            if (value > 20000000) {
                $('#dsitmessage').append("<div class='dsitmessage success'>Maximum amount is 1 crore</div>");
                setTimeout(function() {
